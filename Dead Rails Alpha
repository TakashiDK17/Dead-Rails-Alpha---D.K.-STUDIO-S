local function TLCKPAS_fake_script() -- by @takashi_dk17
	local script = Instance.new('LocalScript', BuscarBonds)

	local button = script.Parent -- by @takashi_dk17
	local runService = game:GetService("RunService")  
	
	local function searchInAllServices()  
		local function checkObject(obj)  
			-- by @takashi_dk17
			if obj:IsA("Model") and obj.Name == "Bond" then  
				local billboardGui = obj:FindFirstChild("ObjectInfo")  
				if billboardGui and billboardGui:IsA("BillboardGui") then  
					-- by @takashi_dk17
					billboardGui.Size = UDim2.new(10, 0, 10, 0)  
					-- by @takashi_dk17
					billboardGui.Enabled = true  
				end  
			end  
		end  
	
		while true do  
			-- by @takashi_dk17
			for _, service in ipairs(game:GetChildren()) do  
				for _, obj in ipairs(service:GetDescendants()) do  
					checkObject(obj)  
				end  
			end  
			wait(1) -- by @takashi_dk17
		end  
	end  
	
	local function onButtonClicked()  
		searchInAllServices()  
	end  
	
	button.MouseButton1Click:Connect(onButtonClicked)  
end
coroutine.wrap(TLCKPAS_fake_script)()
