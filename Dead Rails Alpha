local function LYQNJ_fake_script() -- bytakashi_dk17_17.Hitbox Expander 
	local script = Instance.new('LocalScript', bytakashi_dk17_17)

	local textButton = script.Parent -- by @takashi_dk17  
	local isRunning = false -- Bandera para controlar la ejecución  
	
	textButton.MouseButton1Click:Connect(function()  
		if isRunning then return end -- Evita múltiples ejecuciones simultáneas  
		isRunning = true  
	
		local player = game.Players.LocalPlayer  
		local character = player.Character or player.CharacterAdded:Wait()  
		local playerModel = character:FindFirstChildOfClass("Model") -- by @takashi_dk17  
	
		while true do  
			wait(2) -- by @takashi_dk17
	
			-- by @takashi_dk17
			for _, object in pairs(workspace:GetDescendants()) do  
				if object:IsA("BillboardGui") and object.Name == "ObjectInfo" then  
					local parentModel = object.Parent  
	
					-- by @takashi_dk17
					if parentModel:IsA("Model") and parentModel:FindFirstChild("Humanoid") and parentModel ~= playerModel then  
						local head = parentModel:FindFirstChild("Head")  
						if head then  
							head.Size = Vector3.new(5.95, 5.291, 5.126) -- by @takashi_dk17  
							head.Transparency = 0.3 -- by @takashi_dk17
						end  
					end  
				end  
			end  
		end  
	end)  
end
coroutine.wrap(LYQNJ_fake_script)()
local function KRBOU_fake_script() -- bytakashi_dk17.Verificador 
	local script = Instance.new('LocalScript', bytakashi_dk17)

	local player = game.Players.LocalPlayer  
	local guiName = "ObjectInfo"  
	
	while true do  
		wait(1) -- by @takashi_dk17  
	
		-- Busca el BillboardGui dentro del modelo del jugador  
		local character = player.Character or player.CharacterAdded:Wait()  
		local objectInfoGui = character:FindFirstChild(guiName)  
	
		-- by @takashi_dk17
		if objectInfoGui then  
			objectInfoGui:Destroy()  
		end  
	end  
end
coroutine.wrap(KRBOU_fake_script)()
