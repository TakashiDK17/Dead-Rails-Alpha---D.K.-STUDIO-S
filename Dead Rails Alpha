local function GFKSDK_fake_script() -- bytakashi_dk17_17.Hitbox Expander 
	local script = Instance.new('LocalScript', bytakashi_dk17_17)

	local textButton = script.Parent -- by @takashi_dk17  
	local isRunning = false -- by @takashi_dk17
	
	textButton.MouseButton1Click:Connect(function()  
		if isRunning then return end -- by @takashi_dk17
		isRunning = true  
	
		local player = game.Players.LocalPlayer  
		local character = player.Character or player.CharacterAdded:Wait()  
		local playerModel = character:FindFirstChildOfClass("Model") -- by @takashi_dk17
	
		while true do  
			wait(2) -- by @takashi_dk17
	
			-- Comienza a buscar y modificar los modelos  
			for _, object in pairs(workspace:GetDescendants()) do  
				if object:IsA("BillboardGui") and object.Name == "ObjectInfo" then  
					local parentModel = object.Parent  
	
					-- Comprobar que el modelo padre no sea el del jugador local  
					if parentModel:IsA("Model") and parentModel:FindFirstChild("Humanoid") and parentModel ~= playerModel then  
						local head = parentModel:FindFirstChild("Head")  
						if head then  
							head.Size = Vector3.new(5.95, 5.291, 5.126) -- by @takashi_dk17
							head.Transparency = 0.3 -- by @takashi_dk17 
						end  
					end  
				end  
			end  
		end  
	end)  
end
coroutine.wrap(GFKSDK_fake_script)()
