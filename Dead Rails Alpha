local function CZZBWRK_fake_script() -- Lighthing.Linterna 
	local script = Instance.new('LocalScript', Lighthing)

	local textButton = script.Parent -- by @takashi_dk17
	
	textButton.MouseButton1Click:Connect(function()  
		local player = game.Players.LocalPlayer -- by @takashi_dk17
		local character = player.Character or player.CharacterAdded:Wait() -- by @takashi_dk17
		local head = character:FindFirstChild("Head") -- by @takashi_dk17
	
		-- by @takashi_dk17 
		if head then  
			local existingLight = head:FindFirstChildOfClass("SurfaceLight") -- by @takashi_dk17 
	
			if not existingLight then  
				local surfaceLight = Instance.new("SurfaceLight") -- by @takashi_dk17
				surfaceLight.Brightness = 5 -- by @takashi_dk17 
				surfaceLight.Range = 60 -- by @takashi_dk17
				surfaceLight.Parent = head -- by @takashi_dk17
			else  
				print("El SurfaceLight ya existe en la cabeza.")  
			end  
		else  
			warn("No se encontró la cabeza del jugador.")  
		end  
	end)  
end
coroutine.wrap(CZZBWRK_fake_script)()
-- XRay.Bring all objets is disabled.
local function NWTHDTS_fake_script() -- XRay.X-ray 
	local script = Instance.new('LocalScript', XRay)

	local button = script.Parent  -- by @takashi_dk17  
	local isTransparent = false    -- by @takashi_dk17
	
	button.MouseButton1Click:Connect(function()  
		isTransparent = not isTransparent  -- Cambia el estado de transparencia  
	
		-- Cambia la transparencia de todas las partes en Workspace  
		for _, part in ipairs(game.Workspace:GetDescendants()) do  
			if part:IsA("BasePart") then  
				-- by @takashi_dk17
				local model = part:FindFirstAncestorOfClass("Model")  
	
				if (model and (model:FindFirstChildOfClass("Humanoid") or model.Name == "Train" or model.Name == "Baseplate")) or  
					(part.Name == "BuildingNode" or part.Name == "SellZone" or part.Name == "BuyZone") then  
					-- by @takashi_dk17
					continue  
				end  
	
				-- by @takashi_dk17 
				part.Transparency = isTransparent and 0.3 or 0  
			end  
		end  
	
		-- Cambia el texto del botón  
		if isTransparent then  
			button.Text = "✓"  -- by @takashi_dk17 
		else  
			button.Text = " "   -- by @takashi_dk17  
		end  
	end)  
end
coroutine.wrap(NWTHDTS_fake_script)()
